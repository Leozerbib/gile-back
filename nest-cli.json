{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "root": "apps/gilentry",
  "entryFile": "main",
  "sourceRoot": "apps/gilentry/src",
  "monorepo": true,
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true
  },
  "projects": {
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "gilentry": {
      "type": "application",
      "root": "apps/gilentry",
      "entryFile": "main",
      "sourceRoot": "apps/gilentry/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gilentry/tsconfig.app.json"
      }
    },
    "logger": {
      "type": "application",
      "root": "apps/logger",
      "entryFile": "main",
      "sourceRoot": "apps/logger/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "project": {
      "type": "application",
      "root": "apps/project",
      "entryFile": "main",
      "sourceRoot": "apps/project/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "proto": {
      "type": "library",
      "root": "libs/proto",
      "entryFile": "index",
      "sourceRoot": "libs/proto",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "shared-errors": {
      "type": "library",
      "root": "libs/shared/errors",
      "entryFile": "index",
      "sourceRoot": "libs/shared/errors/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "shared-logger": {
      "type": "library",
      "root": "libs/shared/logger",
      "entryFile": "index",
      "sourceRoot": "libs/shared/logger/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "shared-prisma": {
      "type": "library",
      "root": "libs/shared/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/shared/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "shared-types": {
      "type": "library",
      "root": "libs/shared/types",
      "entryFile": "index",
      "sourceRoot": "libs/shared/types/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "shared-utils": {
      "type": "library",
      "root": "libs/shared/utils",
      "entryFile": "index",
      "sourceRoot": "libs/shared/utils/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "vector": {
      "type": "application",
      "root": "apps/vector",
      "entryFile": "main",
      "sourceRoot": "apps/vector/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "workspace": {
      "type": "application",
      "root": "apps/workspace",
      "entryFile": "main",
      "sourceRoot": "apps/workspace/src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    }
  }
}